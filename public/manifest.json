{
  "name": "Vtabs",
  "version": "1.0.0",
  "description": "Vtabs provide the vertical tabs for the chrome browser",
  "action": {
    "default_title": "Vtabs"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 3,
  "permissions": ["sidePanel", "tabs"],
  "side_panel": {
    "default_path": "index.html"
  }
}
